# A sample Gemfile
source 'https://rubygems.org'
ruby File.read('.ruby-version').strip


group :development do
  gem 'rspec'
  gem 'simplecov'
  gem 'byebug'
  gem 'reek'
  gem 'rubocop', require: false
  gem 'thor'
  gem 'colorize'
end

group :test do
  gem 'codeclimate-test-reporter', require: nil
end
